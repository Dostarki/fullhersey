{
  "name": "spectra-fullstack",
  "version": "2.0.0",
  "description": "SpectraV2 Web3 Fullstack Application",
  "private": true,
  "scripts": {
    "setup": "cd backend && npm install && cd ../frontend && npm install --legacy-peer-deps",
    "prebuild": "npm run setup",
    "build": "cd frontend && npm run build",
    "start": "cd backend && node server.js",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm start\""
  },
  "keywords": [
    "web3",
    "react",
    "node",
    "fullstack"
  ],
  "author": "Trae",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "type": "commonjs"
}
